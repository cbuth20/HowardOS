{
  "name": "howardos",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev:os",
    "dev:os": "cd apps/howard-os-app && npx netlify dev",
    "dev:crm": "cd apps/howard-crm-app && npx netlify dev",
    "dev:uni": "cd apps/howard-university-app && npx netlify dev",
    "build:os": "npm run build -w apps/howard-os-app",
    "build:crm": "npm run build -w apps/howard-crm-app",
    "build:uni": "npm run build -w apps/howard-university-app",
    "type-check": "tsc --noEmit",
    "supabase:gen-types": "supabase gen types typescript --local > packages/ui/types/database.types.ts"
  },
  "dependencies": {
    "busboy": "^1.6.0"
  },
  "devDependencies": {
    "@netlify/functions": "^2.8.0",
    "@types/busboy": "^1.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^9.0.0",
    "supabase": "^2.75.5",
    "typescript": "^5.5.0"
  }
}
